PREFIX dct: <http://purl.org/dc/terms/>
PREFIX task: <http://redpencil.data.gift/vocabularies/tasks/>
PREFIX nfo: <http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#>
PREFIX mu: <http://mu.semte.ch/vocabularies/core/>
INSERT DATA {
    GRAPH <${task.graph}> {
        <${graphContainerUri}> a nfo:DataContainer.
        <${graphContainerUri}> mu:uuid "${graphContainerId}".
        <${graphContainerUri}> task:hasGraph <${filteredGraph}>.
        <${task.task}> task:resultsContainer <${graphContainerUri}>
    }
}
